{
    "version": 2,
    "builds": [
      { "src": "api/*.js", "use": "@vercel/node" }
    ],
    "routes": [
        { 
            "src": "/create-item", 
            "dest": "/api/create-item.js" 
        },
        { 
            "src": "/update-item",
            "dest": "/api/update-item.js" 
        },
        { 
            "src": "/delete-item", 
            "dest": "/api/delete-item.js" 
        }
    ]
  }
  