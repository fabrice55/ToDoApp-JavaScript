{
    "version": 2,
    "builds": [
        { 
            "src": "app.js", 
            "use": "@vercel/node" 
        }
    ],
    "routes": [
        { 
            "src": "/api/(.*)", 
            "dest": "/app.js" 
        },
        { 
            "src": "/(.*)", 
            "dest": "/app.js",
            "headers": { "cache-control": "s-maxage=1, stale-while-revalidate" } 
        }
    ],
    "static": {
        "public": "public"
    } 
  }
  