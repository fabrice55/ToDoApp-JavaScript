{
    "version": 2,
    "public": true,
    "builds": [
        { 
            "src": "app.js", 
            "use": "@vercel/node" 
        }
    ],
    "routes": [
        { 
            "src": "/api/(.*)", 
            "dest": "/app.js" 
        },
        { 
            "src": "/(.*)", 
            "dest": "/app.js" 
        } 
    ]
  }
  